<div class="publickbg">
    <div class="cmax_container">
            <div class="costcontent">
                    <h2 class="pcosthead">{{section.settings.pro_head}}</h2>
                    <p class="pcostdes">{{section.settings.pro_desc}}</p<
            </div>
            <div class="pcostflex">
                {%- for block in section.blocks -%}
                    <div class="costpart">
                        <h2>{{block.settings.cost_head}} </h2>
                        <h3>{{block.settings.cost_price}}</h3>
                        <p>{{block.settings.cost_desc}}</p>
                        <div class="blogbodr">
                            <a href="{{ block.settings.cost_btnurl }}" class="btnspace">{{ block.settings.cost_btn }} 
                              {% render 'icon-arrow' %}
                            </a>
                        </div> 
                    </div>
                {%- endfor -%}
            </div>
    </div>
</div>
{% schema %}
{
  "name": "Program cost",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "text",
      "id": "pro_head",
      "label": "Heading"
    },{
         "type": "text",
      "id": "pro_desc",
      "label": "Description"
    }
  ],
     "blocks": [
  {
    "type": "block",
    "name": "block",
    "settings": [
        {
    "type": "text",
    "id": "cost_head",
    "label": "Heading"
      },
      
          {
    "type": "text",
    "id": "cost_price",
    "label": "Price"
      },
       {
    "type": "text",
    "id": "cost_desc",
    "label": "Description"
       },
       {
        "type": "text",
    "id": "cost_btn",
    "label": "Button"
       },
        {
        "type": "url",
    "id": "cost_btnurl",
    "label": "Button Link"
       }

    ]
  }
    ],
 "presets": [
    {
      "name": "Program cost"
    }
  ]
}
{% endschema %}